{
  "index_name": "prod_rundeck_docs",
  "js_render": false,
  "start_urls": [
    {
      "url": "https://docs.rundeck.com/(?P<version>.*?)/",
      "variables": {
        "version": [
          "docs",
          "4.0.x",
          "5.8.0"
        ]
      }
    }
  ],
  "stop_urls": [
    "https://docs.rundeck.com/.*?/history/[1,2].*?/.*"
  ],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": ".theme-hope-content h1",
        "global": true,
        "default_value": "Documentation"
      },
      "lvl1": ".theme-hope-content h2",
      "lvl2": ".theme-hope-content h3",
      "lvl3": ".theme-hope-content h4",
      "content": {
        "selector": ".theme-hope-content p, .theme-hope-content li, .theme-hope-content pre"
      },
      "title": {
        "selector": ".theme-hope-content h1",
        "global": true
      },
      "description": {
        "selector": "//meta[@name='description']/@content | //meta[@property='og:description']/@content | (//div[contains(@class, 'theme-hope-content')]//p)[1]",
        "type": "xpath",
        "global": true,
        "defaultValue": "Rundeck Documentation"
      },
      "lastUpdated": {
        "selector": "//meta[@property='article:published_time']/@content",
        "type": "xpath",
        "global": true
      }
    }
  },
  "selectors_exclude": [
    ".table-of-contents"
  ],
  "strip_chars": " .,;:#",
  "only_content_level": true,
  "custom_settings": {
    "hitsPerPage": 20,
    "paginationLimitedTo": 1000,
    "distinct": true,
    "attributeForDistinct": "url",
    "attributesForFaceting": [
      "version",
      "lang"
    ],
    "customRanking": [
      "desc(lastUpdated)",
      "desc(weight.page_rank)",
      "desc(weight.level)",
      "asc(weight.position)"
    ],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "description",
      "title",
      "lastUpdated"
    ],
    "attributesToSnippet": [
      "content:150",
      "description:150"
    ],
    "attributesToHighlight": [
      "content",
      "title",
      "description"
    ],
    "searchableAttributes": [
      "title",
      "description",
      "lvl0",
      "lvl1",
      "lvl2",
      "lvl3",
      "content"
    ]
  }
}